@{
    Layout = null;
}
<head>
    <link rel="stylesheet" href="https://cdn.staticfile.org/twitter-bootstrap/4.3.1/css/bootstrap.min.css">
    <script src="https://cdn.staticfile.org/jquery/3.2.1/jquery.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <style type="text/css">
        table, th {
            color: #FFFFFF;
            font-weigh: bold;
            text-align: center;
        }

        table, tr {
            color: #000000;
            font-weigh: bold;
        }

        body {
            background: #53548A fixed top;
        }
    </style>
</head>
<div id="app" class="carousel slide" data-ride="carousel" style="width:100%" data-interval="6000">
    <div>&nbsp;</div>
    <div style="color:#FFFFFF;font-size:large">&nbsp;&nbsp;&nbsp;&nbsp;名字</div>
    <div class="container" style="width:100%">
        <div class="d-flex flex-row-reverse" style="width:100%">
            <div class="p-2"><button type="button" class="btn btn-default" style="background-color:#FFFF00">学院信息</button></div>
            <div class="p-2"><button type="button" class="btn btn-default" style="background-color:#FFFF00">业务添加</button></div>
        </div>
        <div></div>
    </div>
    <div>&nbsp;</div>
    <table v-show="contactsList.length" class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>No</th>
                <th>分院</th>
                <th>收入</th>
                <th>支出</th>
                <th>利润</th>
                <th>分成比例</th>
                <th>备注</th>
                <th>个人收入</th>
            </tr>
        </thead>
        <tr v-for="(contact,index) in contactsList">
            <td>{{contact.Id}}</td>
            <td>{{contact.Name}}</td>
            <td>{{contact.Income}}</td>
            <td>{{contact.Amount}}</td>
            <td>{{contact.Profit}}</td>
            <td>{{contact.AmountReMark}}</td>
            <td>{{contact.OtherReMark}}</td>
            <td>{{contact.OtherReMark}}</td>            
        </tr>
    </table>
    <div class="container" style="width:40%;background-color:#FFFF00">
        <div class="d-flex flex-row" style="width:40%">
            总计：
        </div>
    </div>
    <div class="container" style="width:100%">
        <div class="d-flex flex-row-reverse" style="width:100%">
            <div class="p-2"><button type="button" class="btn btn-default" style="background-color:#FFFF00">导出表格</button></div>
        </div>
        <div></div>
    </div>
    <script src="~/js/vue.js"></script>
    <script src="~/js/axios.js"></script>
    <script>

            var app2 = new Vue({
                el: '#app',
                data: {
                    message: 'Hello Vue!',
                    contactsList: [
                        { Id: "1", Name: "aa课",Income: "132", Amount: "60000", AmountReMark: "支出1：11", OtherReMark: "特记事项", Profit: "100" },
                        { Id: "2", Name: "bb课",Income: "132", Amount: "", AmountReMark: "", OtherReMark: "", Profit: "" },
                        { Id: "3", Name: "cc课",Income: "132", Amount: "", AmountReMark: "", OtherReMark: "", Profit: "" }]
                }
            });
    </script>
